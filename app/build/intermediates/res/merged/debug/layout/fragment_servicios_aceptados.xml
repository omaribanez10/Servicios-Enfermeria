<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.omar10ibanez.serviciosenfermeria.enfermera.ServiciosAceptados"
    android:background="@drawable/backgground_app_nurse_perfil">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical">
        <TextView
            android:id="@+id/textViewServiciosAceptados"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_gravity="center_horizontal"
            android:text="Servicios Aceptados"
            android:textColor="#2271b3"
            android:textSize="25dp"
            android:layout_margin="10dp"
            android:textStyle="bold"/>
    </LinearLayout>
</LinearLayout>
